{
  "name": "simple.dmw",
  "description": "The simple document management workflow",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "build:stage": "env-cmd -f .env.stage craco build",
    "build:prod": "env-cmd -f .env.production craco build",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@craco/craco": "6.4.5",
    "@reduxjs/toolkit": "1.4.0",
    "@thyrith/momentkh": "2.5.3",
    "antd": "4.21.6",
    "axios": "0.21.1",
    "craco-less": "2.0.0",
    "fontfaceobserver": "2.1.0",
    "i18next": "21.8.14",
    "i18next-browser-languagedetector": "6.0.1",
    "jspdf": "2.5.1",
    "nanoid": "3.1.23",
    "print-js": "1.6.0",
    "quill-delta-to-html": "0.12.1",
    "react": "17.0.2",
    "react-app-polyfill": "1.0.6",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.1",
    "react-icons": "4.4.0",
    "react-quill": "2.0.0",
    "react-redux": "7.2.1",
    "react-router-dom": "^5.3.3",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "styled-components": "5.1.0",
    "web-vitals": "0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.2",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "25.1.4",
    "@types/node": "14.6.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "6.1.5",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "16.9.3",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@types/styled-components": "5.1.3",
    "@types/testing-library__jest-dom": "5.9.2",
    "@types/webpack": "4.41.22",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "chalk": "4.1.0",
    "cross-env": "7.0.2",
    "env-cmd": "10.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react-hooks": "4.1.0",
    "husky": "4.2.5",
    "i18next-scanner": "2.11.0",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.3.0",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "prettier": "2.1.1",
    "react-scripts": "4.0.3",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "serve": "11.3.2",
    "shelljs": "0.8.4",
    "stylelint": "13.7.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "9.0.0",
    "typescript": "4.7.4"
  }
}
